{
    "name": "sukohi/julius",
    "description": "A PHP package mainly developed for Laravel to manage calendar and events.",
    "authors": [
        {
            "name": "Sukohi",
            "email": "capilano.sukohi@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Sukohi\\Julius\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sukohi\\Julius\\JuliusServiceProvider"
            ],
            "aliases": {
                "Julius": "Sukohi\\Julius\\Facades\\Julius"
            }
        }
    },
    "minimum-stability": "stable"
}
